package syo1;
import java.util.*;


public class Main {
    public static void main(String[] args) {
        // 自分の得意な言語で
        // Let's チャレンジ！！

        Scanner sc = new Scanner(System.in);
        int N = sc.nextInt();//回転する回数
        int H = sc.nextInt();//マス目の縦の大きさ
        int W = sc.nextInt();//マス目の横の大きさ
        int[][] map = new int[H][W];
        int sy = sc.nextInt();//最初にサイコロを置く行
        int sx = sc.nextInt();//最初にサイコロを置く列
        map[sy-1][sx-1] = 6;
        String sp = sc.next();
        String[] s = sp.split("");//移動方法
        int dice = 6;
        String compass = "South";


        for(int i = 0; i < N; i++){
            switch(dice){

                case 1:
                    switch(compass){
                        case "North":
                            switch(s[i]){
                                case "U":
                                    sy -= 1;
                                    dice = 2;
                                    map[sy-1][sx-1] = 2;
                                    break;

                                case "D":
                                    sy += 1;
                                    dice = 5;
                                    map[sy-1][sx-1] = 5;
                                    break;

                                case "L":
                                    sx -= 1;
                                    dice = 4;
                                    map[sy-1][sx-1] = 4;
                                    break;

                                case "R":
                                    sx += 1;
                                    dice = 3;
                                    map[sy-1][sx-1] = 3;
                                    break;
                            }
